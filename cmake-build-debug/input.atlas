CC This is a program that will search for a given pattern in a string of text


DEFINE length(s:STRING,) $ NUM {

    NUM len = 0;
    FOR [s] {
        len = len + 1;
    };

    RETURN len;
};


INPUT STRING source;
INPUT STRING target;

NUM lenOfInput =  @length(source,);

NUM it = 0;
FOR [lenOfInput] {
    STRING letter = [it..it]source;
    WRITE letter;
    it = it + 1;

};



